pipeline 
{
    agent any
stages
{
    stage ('scm checkout')
    {
        steps { git branch: 'master', url: 'https://github.com/umesh8301/jenkins_script.git'}
    }

    stage ('run unit test framework')
    {
        steps {  jdk: 'JAVA_HOME', maven: 'MNV_HOME') {
        sh 'mvn test'
                                                                                                                               } 
              }
    
     }


}
}
